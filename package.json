{
  "name": "photographer-crm",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run copy-pdf-worker && vite",
    "build": "npm run copy-pdf-worker && tsc && vite build",
    "copy-pdf-worker": "node -e \"const isWindows = process.env.AMBIENTE === 'desenvolvimento' || process.platform === 'win32'; if (isWindows) { const fs = require('fs'); if (!fs.existsSync('public')) fs.mkdirSync('public'); fs.copyFileSync('node_modules/pdfjs-dist/build/pdf.worker.min.mjs', 'public/pdf.worker.min.js'); } else { require('child_process').execSync('mkdir -p public && cp node_modules/pdfjs-dist/build/pdf.worker.min.mjs public/pdf.worker.min.js'); }\"",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@react-oauth/google": "^0.12.1",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.7.9",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dompurify": "^3.2.6",
    "jspdf": "^2.5.1",
    "lucide-react": "^0.344.0",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.8.69",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-datepicker": "^7.5.0",
    "react-dom": "^18.3.1",
    "react-modal": "^3.16.3",
    "react-pdf": "^9.2.1",
    "react-router-dom": "^6.22.3",
    "react-toastify": "^11.0.3",
    "recharts": "^2.12.2",
    "sweetalert2": "^11.17.2",
    "sweetalert2-react-content": "^5.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
